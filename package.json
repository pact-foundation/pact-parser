{
  "name": "pact-parser",
  "version": "0.1.0",
  "description": "This application is devoted to parsing json files that represent [pacts](https://github.com/pact-foundation/pact-specification), spawning express server and replaying responses for requests read from pact files.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "lint": "eslint .",
    "test": "mocha test/*",
    "test:cover": "istanbul cover _mocha -- test/*",
    "test:travis": "./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- test/*",
    "debug:test": "mocha --debug-brk test/*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ichyr/pact-parser.git"
  },
  "keywords": [
    "pact",
    "consumer driven contract",
    "testing",
    "js"
  ],
  "author": "Ivan Chyr <zmii@plast.org.ua>",
  "contributors": [
    "Ivan Chyr <zmii@plast.org.ua>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ichyr/pact-parser/issues"
  },
  "homepage": "https://github.com/ichyr/pact-parser",
  "devDependencies": {
    "chai": "^3.4.1",
    "codeclimate-test-reporter": "^0.3.1",
    "eslint": "^2.8.0",
    "gulp": "^3.9.0",
    "istanbul": "^0.4.3",
    "mocha": "^2.3.4",
    "nodemon": "^1.8.1",
    "rimraf": "^2.5.1",
    "sinon": "^1.17.2",
    "supertest": "^1.2.0"
  },
  "dependencies": {
    "body-parser": "^1.15.0",
    "bunyan": "^1.8.1",
    "express": "^4.13.4",
    "glob": "^6.0.4",
    "underscore": "^1.8.3",
    "url": "^0.11.0",
    "yargs": "^13.2.4"
  },
  "engines": {
    "node": ">=0.12"
  }
}
